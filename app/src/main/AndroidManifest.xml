<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidgamedev.com.reminiscence">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/reminiscence"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/reminiscence"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ImageGridActivity" />
        <activity
            android:name=".ImageViewerActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name=".ImageDescriptActivity" />
        <activity android:name=".SignUpActivity"></activity>
        <activity
            android:name=".UploadMemoryActivity"
            android:theme="@style/DialogUsingTheme"></activity>
        <activity android:name=".ChangePasswordActivity"></activity>
    </application>

</manifest>